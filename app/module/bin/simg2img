# Kakathic

if [ "$1" == '-h' ] || [ "$1" == '--help' ] || [ -z "$1" ];then
$TERMUX/bin/simg2img --help
exit 1
fi

if [ -n "$1" ] && [ -z "$2" ];then
$TERMUX/bin/simg2img "$1" "${1}_r"
[ -f "${1}_r" ] && mv "${1}_r" "${1}" || abort "Convert img file simg2img error"
else
$TERMUX/bin/simg2img "$@"
fi
